{
  "name": "@syncstuff/app",
  "author": {
    "name": "Involvex"
  },
  "repository": {
    "url": "https://github.com/involvex/syncstuff.git"
  },
  "private": true,
  "version": "0.0.15",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "bun run test.unit",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "serve": "ionic serve",
    "start:signaling": "node signaling-server.cjs",
    "android": " ionic cap run android -l --external",
    "gradle:stop": "cd android && ./gradlew.bat --stop",
    "gradle:clean": "cd android && ./gradlew.bat clean",
    "electron:dev": "concurrently \"bun run start:signaling\" \"bun run dev\" \"bun run --cwd electron dev\"",
    "electron:build": "bun run build && pnpm --filter @syncstuff/electron build",
    "electron:start": "bun run --cwd electron start"
  },
  "dependencies": {
    "@syncstuff/ui": "workspace:*",
    "@aparajita/capacitor-dark-mode": "6.0.0",
    "@byteowls/capacitor-filesharer": "7.0.0",
    "@capacitor-community/electron": "5.0.1",
    "@capacitor-mlkit/barcode-scanning": "8.0.0",
    "@capacitor/android": "8.0.0",
    "@capacitor/app": "8.0.0",
    "@capacitor/clipboard": "8.0.0",
    "@capacitor/core": "8.0.0",
    "@capacitor/device": "8.0.0",
    "@capacitor/filesystem": "8.0.0",
    "@capacitor/haptics": "8.0.0",
    "@capacitor/ios": "8.0.0",
    "@capacitor/keyboard": "8.0.0",
    "@capacitor/network": "8.0.0",
    "@capacitor/preferences": "8.0.0",
    "@capacitor/push-notifications": "8.0.0",
    "@capacitor/share": "8.0.0",
    "@capacitor/status-bar": "8.0.0",
    "@eslint/js": "9.39.2",
    "@ionic/react": "8.7.16",
    "@ionic/react-router": "8.7.16",
    "@ionic/storage": "4.0.0",
    "@types/cypress": "1.1.6",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "capacitor-native-settings": "7.0.2",
    "capacitor-zeroconf": "4.0.0",
    "crypto-js": "4.2.0",
    "ionicons": "8.0.13",
    "megajs": "1.3.9",
    "qrcode.react": "4.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "simple-peer": "9.11.1",
    "socket.io": "4.8.3",
    "socket.io-client": "4.8.3",
    "uuid": "13.0.0",
    "zustand": "5.0.9",
    "react-native-svg": "^15.0.0",
    "react-native-web": "^0.21.2"
  },
  "devDependencies": {
    "@capacitor/assets": "3.0.5",
    "@capacitor/cli": "8.0.0",
    "react-native-worklets": "^0.7.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/crypto-js": "4.2.2",
    "@types/gapi": "0.0.47",
    "@types/gapi.client.drive": "3.0.15",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/simple-peer": "9.11.9",
    "@types/uuid": "11.0.0",
    "@vitejs/plugin-legacy": "7.2.1",
    "@vitejs/plugin-react": "5.1.2",
    "concurrently": "9.2.1",
    "cypress": "15.8.2",
    "eslint": "9.39.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.26",
    "globals": "17.0.0",
    "jsdom": "27.4.0",
    "prettier": "3.7.4",
    "terser": "5.44.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0",
    "vite": "7.3.1",
    "vite-plugin-node-polyfills": "0.24.0",
    "vitest": "4.0.16",
    "@syncstuff/network-types": "workspace:*"
  },
  "overrides": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0"
  },
  "description": "An App to synchronize your files to all your devices."
}
