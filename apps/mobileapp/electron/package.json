{
  "name": "@syncstuff/electron",
  "version": "0.0.11",
  "description": "Syncstuff Electron Desktop App",
  "author": {
    "name": "Involvex"
  },
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=16384 electron-builder --config.asar=true",
    "dev": "electron . --dev",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "electron-is-dev": "^3.0.1"
  },
  "dependencies": {
    "@syncstuff/ui": "workspace:*",
    "electron-debug": "^4.1.0",
    "electron-reloader": "^1.2.3",
    "electron-unhandled": "^5.0.0",
    "is-extglob": "^2.1.1"
  }
}
