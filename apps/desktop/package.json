{
  "name": "@involvex/syncstuff-cli",
  "version": "0.0.7",
  "type": "module",
  "homepage": "https://syncstuff-web.involvex.workers.dev/",
  "sponsor": {
    "url": "https://github.com/sponsors/involvex"
  },
  "author": {
    "name": "involvex"
  },
  "bin": {
    "syncstuff": "dist/cli.js"
  },
  "scripts": {
    "dev": "bun run src/cli/index.ts",
    "build": "bun build --target node --outfile dist/cli.js src/cli/index.ts",
    "lint": "eslint src ",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "prebuild": "bun run format && bun run lint:fix && bun run typecheck",
    "prepublish": "bun run build"
  },
  "dependencies": {
    "@types/node": "25.0.3",
    "chalk": "5.6.2",
    "inquirer": "13.1.0",
    "ora": "9.0.0",
    "boxen": "8.0.1",
    "table": "6.9.0"
  },
  "devDependencies": {
    "@eslint/css": "0.14.1",
    "@eslint/js": "9.39.2",
    "@eslint/json": "0.14.0",
    "@types/bun": "1.3.5",
    "@types/inquirer": "9.0.9",
    "eslint": "9.39.2",
    "globals": "17.0.0",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "4.3.0",
    "typescript-eslint": "8.52.0"
  },
  "peerDependencies": {
    "typescript": "5.9.3"
  }
}
