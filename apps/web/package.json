  "private": true,
  "scripts": {
    "build": "remix vite:build",
    "cf-typegen": "wrangler types",
    "check": "tsc && remix vite:build && wrangler deploy --dry-run",
    "deploy": "bun run build && wrangler deploy -c wrangler.toml",
    "dev": "remix vite:dev",
    "wrangler:dev": "wrangler dev -c wrangler.toml",
    "wrangler:debug": "wrangler dev -c wrangler.toml --minify false -r --env-file .env --env development --log-level debug",
    "lint": "eslint . --ignore-pattern build --ignore-pattern .cache --ignore-pattern .wrangler",
    "preview": "npm run build && wrangler dev",
    "start": "wrangler dev",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "lint:fix": "eslint . --fix --ignore-pattern build  --ignore-pattern .cache --ignore-pattern .wrangler",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "sideEffects": false,
  "type": "module"
}
