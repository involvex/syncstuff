{
  "name": "@syncstuff/ui",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist-cjs/index.js"
    },
    "./tamagui.config": {
      "types": "./dist/tamagui.config.d.ts",
      "import": "./dist/tamagui.config.js",
      "require": "./dist-cjs/tamagui.config.js"
    },
    "./dist-cjs/tamagui.config.cjs": "./dist-cjs/tamagui.config.cjs"
  },
  "files": [
    "dist",
    "dist-cjs",
    "src",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && tsc --project tsconfig.cjs.json && node copy-css.mjs && node copy-config-cjs.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "lucide-react": "^0.479.0",
    "tailwind-merge": "^2.5.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@eslint/json": "0.14.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "eslint": "9.39.2",
    "glob": "13.0.0",
    "globals": "17.0.0",
    "prettier": "3.7.4",
    "react-native": "^0.83.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.52.0"
  }
}
