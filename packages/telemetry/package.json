{
  "name": "@syncstuff/telemetry",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "deploy": "wrangler deploy -c wrangler.toml",
    "dev": "wrangler dev -c wrangler.toml --env-file .env",
    "dev:local": "wrangler dev -c wrangler.toml -l --env-file .env",
    "start": "wrangler dev",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@microlabs/otel-cf-workers": "^1.0.0-rc.20",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/core": "^1.17.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.43.0",
    "@opentelemetry/otlp-exporter-base": "^0.43.0",
    "@opentelemetry/otlp-transformer": "^0.43.0",
    "@opentelemetry/resources": "^1.17.1",
    "@opentelemetry/sdk-trace-base": "^1.17.1",
    "@opentelemetry/semantic-conventions": "^1.17.1",
    "deepmerge": "^4.3.1",
    "ts-checked-fsm": "^1.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260108.0",
    "@eslint/js": "9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "globals": "17.0.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.57.0"
  }
}
